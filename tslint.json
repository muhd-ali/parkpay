{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js"
      ]
    },
    "rules": {
      "interface-name" : [true, "never-prefix"],
      "member-ordering": false,
      "max-classes-per-file": false,
      "semicolon": [true, "always"],
      "no-bitwise": false,
      "no-console": false
    }
  }